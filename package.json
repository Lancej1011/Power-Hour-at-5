{
  "name": "phat5",
  "productName": "PHat5",
  "private": false,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "build": "tsc -b && vite build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:web": "vite build --config vite.config.web.ts && node scripts/fix-sw.js",
    "build:web:prod": "cross-env NODE_ENV=production npm run build:web",
    "preview:web": "vite preview --config vite.config.web.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:build": "npm run build:prod && electron-builder",
    "electron:dist": "npm run build:prod && electron-builder --publish=never",
    "electron:pack": "npm run build:prod && electron-builder --dir",
    "electron:publish": "npm run build:prod && electron-builder --publish=always",
    "release": "node scripts/build-release.js",
    "release:draft": "DRAFT_RELEASE=true node scripts/build-release.js",
    "release:prerelease": "PRERELEASE=true node scripts/build-release.js",
    "clean": "rimraf dist dist-electron dist-web",
    "prebuild": "npm run clean",
    "analyze": "npm run build -- --analyze",
    "build:production": "node scripts/build-production.js",
    "build:installer": "node scripts/build-installer.cjs",
    "build:installer:win": "BUILD_PLATFORMS=win node scripts/build-installer.cjs",
    "build:installer:mac": "BUILD_PLATFORMS=mac node scripts/build-installer.cjs",
    "build:installer:linux": "BUILD_PLATFORMS=linux node scripts/build-installer.cjs",
    "build:installer:all": "BUILD_PLATFORMS=win,mac,linux node scripts/build-installer.cjs",
    "copy-installer": "node scripts/copy-installer.cjs",
    "fix-firebase": "node scripts/fix-firebase-and-rebuild.cjs",
    "setup-env": "node scripts/setup-env.js",
    "deploy:firebase": "npm run build:web:prod && firebase deploy --only hosting",
    "deploy:netlify": "npm run build:web:prod && netlify deploy --prod --dir=dist-web",
    "deploy:vercel": "npm run build:web:prod && vercel --prod"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@types/react-color": "^3.0.13",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "archiver": "^7.0.1",
    "audiobuffer-to-wav": "^1.0.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.3.9",
    "extract-zip": "^2.0.1",
    "ffmpeg-static": "^5.2.0",
    "firebase": "^11.9.0",
    "google-auth-library": "^9.14.1",
    "googleapis": "^144.0.0",
    "howler": "^2.2.4",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "music-metadata": "^11.2.1",
    "re-resizable": "^6.11.2",
    "react": "^19.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.5.3",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "react-youtube": "^10.1.0",
    "wavefile": "^11.0.0",
    "web-audio-api": "^0.2.2",
    "yt-dlp-wrap": "^2.3.12",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@eslint/js": "^9.22.0",
    "@types/archiver": "^6.0.3",
    "@types/extract-zip": "^2.0.0",
    "@types/howler": "^2.2.12",
    "@types/lodash": "^4.17.13",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-youtube": "^7.6.2",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.1.0",
    "electron-builder": "^24.13.3",
    "electron-updater": "^6.3.9",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "rimraf": "^6.0.1",
    "terser": "^5.40.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-pwa": "^0.21.2",
    "wait-on": "^8.0.3"
  },
  "description": "PHat5 - The ultimate Power Hour music mixing application. Create custom playlists, extract clips, and host the perfect Power Hour party.",
  "main": "main.cjs",
  "keywords": [
    "power hour",
    "music",
    "party",
    "playlist",
    "audio",
    "mixing",
    "electron",
    "music player"
  ],
  "author": "PHat5 Development Team",
  "license": "MIT",
  "homepage": "https://github.com/your-username/phat5",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/phat5.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/phat5/issues"
  }
}
